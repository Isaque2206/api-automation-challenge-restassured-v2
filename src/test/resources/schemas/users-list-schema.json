{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "quantidade",
    "usuarios"
  ],
  "properties": {
    "quantidade": {
      "type": "integer"
    },
    "usuarios": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "nome",
          "email",
          "_id"
        ],
        "properties": {
          "nome": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "_id": {
            "type": "string"
          },
          "administrador": {
            "type": "string"
          }
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}